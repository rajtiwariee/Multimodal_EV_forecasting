# Docker Compose Project Name (optional, defaults to directory name)
COMPOSE_PROJECT_NAME=ev_scooter_mlops

# PostgreSQL
POSTGRES_DB=scooter_db
POSTGRES_USER=scooter_user
POSTGRES_PASSWORD=scooter_password

# Kafka
KAFKA_BROKER_ID=1
KAFKA_PORT_HOST=29092 # Port exposed to the host machine
KAFKA_PORT_INTERNAL=9092 # Port for inter-container communication

# Redis
REDIS_PORT=6379

# Add other environment variables as needed, e.g., API keys
# OPEN_METEO_API_KEY=your_api_key_here